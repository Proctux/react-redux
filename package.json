{
  "name": "boilerplate-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true node scripts/customized-config start",
    "build": "SKIP_PREFLIGHT_CHECK=true node scripts/customized-config build",
    "test": "yarn lint && yarn jest",
    "lint": "yarn eslint && yarn stylelint",
    "eslint": "NODE_ENV=test eslint src tests *.js",
    "stylelint": "NODE_ENV=test stylelint 'src/**/*.css'",
    "eject": "react-scripts eject"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(css)$": "identity-obj-proxy",
      "^.+\\.(png|jpg|jpeg|svg|txt|ico)$": "<rootDir>/tests/empty-module.js",
      "^_modules/(.*)": "<rootDir>/src/modules/$1",
      "^_components/(.*)": "<rootDir>/src/components/$1",
      "^_config/(.*)": "<rootDir>/src/config/$1",
      "^_services/(.*)": "<rootDir>/src/services/$1",
      "^_models/(.*)": "<rootDir>/src/models/$1",
      "^_models": "<rootDir>/src/models",
      "^_views/(.*)": "<rootDir>/src/views/$1",
      "^_utils/(.*)": "<rootDir>/src/utils/$1",
      "^_styles/(.*)": "<rootDir>/src/styles/$1",
      "^_store/(.*)": "<rootDir>/src/store/$1",
      "^_hocs/(.*)": "<rootDir>/src/hocs/$1",
      "^_middlewares/(.*)": "<rootDir>/src/middlewares/$1",
      "^_assets/(.*)": "<rootDir>/src/assets/$1",
      "\\.worker.js": "<rootDir>/tests/__mocks__/workerMock.js",
      "^_privateDependencies/(.*)": "<rootDir>/private-dependencies/$1"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "setupFiles": [
      "./tests/config.test.js"
    ],
    "testPathIgnorePatterns": [
      "./tests/config.test.js",
      "./private-dependencies",
      "<rootDir>[/\\\\](dist|dist-server|node_modules|.storybook)[/\\\\]"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.1.1",
    "axios": "^0.18.0",
    "humps": "^2.0.1",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.2",
    "proxyquire": "^2.0.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "2.0.0-next.3e165448",
    "redux": "^4.0.0",
    "redux-define": "^1.1.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "rewire": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-custom-media": "^6.0.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "prettier": "^1.13.7",
    "raf-polyfill": "^1.0.0",
    "redux-devtools-extension": "^2.13.5",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-sprite-loader": "^3.8.0",
    "webpack": "^4.14.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
